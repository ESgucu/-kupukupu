<!-- app/views/memberships/edit.html.erb -->
<h1>Edit Membership</h1>
<%= simple_form_for(@membership) do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :travel_date %>
    <%= f.input :return_date %>
    <%= f.input :departure %>
  </div>
  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>
